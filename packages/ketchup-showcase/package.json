{
  "name": "ketchup-showcase",
  "version": "0.0.1",
  "keywords": [
    "Sme.UP",
    "KetchUP showcase",
    "pwa",
    "custom elements",
    "components",
    "Vue.js"
  ],
  "description": "Sme.UP's KetchUP web components library showcase based on Vuetify",
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:bundle": "rimraf dist && vue-cli-service build --target lib --name vueup ./src/components/index.js",
    "build:label": "rimraf dist && vue-cli-service build --target lib --name SmeupLabel src/components/SmeupLabel.vue",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "start": "vue-cli-service serve",
    "test:e2e:open": "start-test 4000 cy:open",
    "test:e2e:run": "start-test 4000 cy:run",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@mdi/font": "^2.8.94",
    "@vaadin/vaadin-upload": "^4.2.2",
    "element-ui": "^2.4.8",
    "ketchup": "^1.0.3-SNAPSHOT",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-material-design-icons": "^2.3.0",
    "material-design-icons-iconfont": "^4.0.5",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.3",
    "vuetify": "^1.5.5"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-prettier": "^3.0.5",
    "@vue/eslint-config-typescript": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "cypress": "^4.8.0",
    "cypress-shadow-dom": "^1.0.0",
    "jest-html-reporter": "^2.4.2",
    "jest-junit": "^5.2.0",
    "node-sass": "^4.12.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "start-server-and-test": "^1.7.10",
    "ts-jest": "^23.0.0",
    "typescript": "3.9.3",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.5"
  },
  "vaadin": {
    "disableUsageStatistics": true
  }
}
